{"name":"npm-atomic-publish","description":"publish to npm with a single http request","version":"0.0.9","homepage":"https://github.com/dominictarr/npm-atomic-publish","repository":"https://github.com/dominictarr/npm-atomic-publish","dependencies":{"npmconf":"~0.1.6","request":"~2.27.0","npm-registry-client":"~0.2.30","mkdirp":"~0.3.5","shasum":"~1.0.0","npmd-pack":"~1.0.0","concat-stream":"~1.2.1"},"scripts":{"test":"set -e; for t in test/*.js; do node $t; done"},"bin":{"npm-atomic-publish":"./bin.js"},"license":"MIT","versions":[{"number":"0.0.1","date":"2013-12-11T15:03:02.412Z"},{"number":"0.0.2","date":"2013-12-11T15:05:44.383Z"},{"number":"0.0.3","date":"2013-12-11T15:22:08.067Z"},{"number":"0.0.4","date":"2013-12-11T15:24:20.053Z"},{"number":"0.0.5","date":"2013-12-11T18:18:09.867Z"},{"number":"0.0.7","date":"2013-12-14T11:06:16.679Z"},{"number":"0.0.8","date":"2013-12-14T11:12:53.559Z"},{"number":"0.0.9","date":"2014-01-24T15:03:42.353Z"}],"readme":"var fs = require('fs')\n\nmodule.exports = function readme (pkgdir, cb) {\n  fs.readdir(pkgdir, function (err, files) {\n    var rfile = files.filter(function (file) {\n      return /^readme(\\.(md|markdown|txt))?$/i.test(file)\n    }).sort()[0]\n    if (!rfile) cb()\n    else fs.readFile(rfile, 'utf8', function (err, src) {\n      cb(err, src, rfile)\n    })\n  })\n}\n\n","created":"2013-12-11T15:03:02.412Z","modified":"2014-01-24T15:03:42.353Z","lastPublisher":{"name":"dominictarr","email":"dominic.tarr@gmail.com"},"owners":[{"name":"dominictarr","email":"dominic.tarr@gmail.com"}],"other":{"_attachments":{},"_from":".","_id":"npm-atomic-publish","_npmUser":{"name":"dominictarr","email":"dominic.tarr@gmail.com"},"_npmVersion":"1.3.21","_rev":"1-c966dd9c82a646e5acfc709f30d28a80","author":{"name":"Dominic Tarr","email":"dominic.tarr@gmail.com","url":"dominictarr.com"},"bugs":{"url":"https://github.com/dominictarr/npm-atomic-publish/issues"},"directories":{},"dist-tags":{"latest":"0.0.9"},"dist":{"shasum":"25b5699f