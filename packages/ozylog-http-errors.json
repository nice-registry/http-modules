ript\nimport {BadRequestError} from 'ozylog-http-errors';\n\nexport function validate(req, res, next) {\n  if (!req.body.name) return next(new BadRequestError());\n}\n```\n\n## License\nMIT\n","created":"2016-12-29T02:48:34.568Z","modified":"2017-03-07T09:30:24.499Z","lastPublisher":{"name":"cahyap","email":"cahya@monelog.com"},"owners":[{"name":"cahyap","email":"cahya@monelog.com"}],"other":{"_attachments":{},"_from":".","_id":"ozylog-http-errors","_nodeVersion":"7.4.0","_npmOperationalInternal":{"host":"packages-12-west.internal.npmjs.com","tmp":"tmp/ozylog-http-errors-1.0.10.tgz_1488879024260_0.9427935914136469"},"_npmUser":{"name":"cahyap","email":"cahya@monelog.com"},"_npmVersion":"4.1.1","_rev":"19-831b74c48a94c4563d2648e43092b76d","_shasum":"545548e81e318d0cc8e73990e941f210638da81d","author":{"name":"Cahya Pribadi","email":"ozylog@gmail.com"},"bugs":{"url":"https://github.com/ozylog/ozylog-http-errors/issues"},"directories":{},"dist-tags":{"latest":"1.0.10"},"dist":{"shasum":"545548e81e318d0cc8e73990e941f210638da81d","tarball":"https://registry.npmjs.org/ozylog-http-errors/-/ozylog-http-errors-1.0.10.tgz"},"maintainers":[{"name":"cahyap","email":"cahya@monelog.com"}],"readmeFilename":"README.md","time":{"modified":"2017-03-07T09:30:24.499Z","created":"2016-12-29T02:48:34.568Z","1.0.0":"2016-12-29T02:48:34.568Z","1.0.1":"2016-12-30T01:02:08.277Z","1.0.2":"2017-01-03T11:44:25.624Z","1.0.3":"2017-01-03T11:46:13.688Z","1.0.4":"2017-02-03T05:17:52.110Z","1.0.5":"2017-02-03T05:22:26.874Z","1.0.6":"2017-02-03T05:29:04.222Z","1.0.7":"2017-02-07T09:46:25.126Z","1.0.8":"2017-02-07T09:52:07.737Z","1.0.9":"2017-03-07T09:27:47.283Z","1.0.10":"2017-03-07T09:30:24.499Z"}}}